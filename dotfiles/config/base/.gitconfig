	email = rrsadler@gmail.com
	name = Ryan Sadler

[core]
        pager = delta --color-only --line-numbers --dark --features=interactive

[delta]
    plus-color = "#012800"
    minus-color = "#340001"
    syntax-theme = Monokai Extended
    features = decorations

[interactive]
    diffFilter = delta --color-only --features=interactive

[pull]
	ff = only

[push]
	default = current
  autoSetupRemote = true
